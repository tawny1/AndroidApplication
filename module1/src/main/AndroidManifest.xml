<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.tawny.color_state_list">

    <uses-permission android:name = "android.permission.GET_TASKS"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/wei"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/wei"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".ColorStateListActivity"/>

        <activity android:name="com.tawny.cardview_color.CardViewActivity" android:launchMode="singleTop"/>

        <activity android:name="com.tawny.bottom_navigation.BottomNavigationActivity" android:launchMode="singleTop"/>

        <activity android:name="com.tawny.coordinator_layout.CoordinatorActivity" android:launchMode="singleTop"/>

        <activity android:name="com.tawny.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.tawny.retrofit_package.NetActivity">

        </activity>


    </application>

</manifest>